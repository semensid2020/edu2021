<h1>Новая проверка</h1>

  <%= form_for(model: checking) do |form| %>

    <div class="form-group">
      <%= form.file_field :student_work, accept: 'application/pdf', class: 'file-upload'%>
    </div>

    <div class="form-group">
      <%= form.submit class: 'btn btn-primary' %>
      <%= link_to "Cancel", checkings, class: "btn btn-link" %>

    </div>

  <% end %>
